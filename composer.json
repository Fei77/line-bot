{
    "name": "fei77/line-bot",
    "version": "1.0",
    "type": "library",
    "description": "Laravel Line Bot package",
    "keywords": [
        "Fei77",
        "LineBot"
    ],
    "homepage": "https://github.com/fei77/line-bot",
    "license": "MIT",
    "authors": [
        {
            "name": "Fei77",
            "email": "fei@feilink.net",
            "homepage": "https://feilink.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^5.6",
        "linecorp/line-bot-sdk" : "^2.3"
    },
    "autoload": {
        "psr-4": {
            "Fei77\\LineBot\\": "src"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
